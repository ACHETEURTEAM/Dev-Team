<?xml version="1.0"?>
<!--
 
-->
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Event/etc/events.xsd">
    <event name="csproduct_vproducts_edit_action">
        <observer name="bundle_observer" instance="Ced\CsProduct\Observer\SetAttributeTabBlockObserver"/>
    </event>
    <event name="csproduct_vproducts_new_action">
        <observer name="bundle_observer" instance="Ced\CsProduct\Observer\SetAttributeTabBlockObserver"/>
    </event>
    <event name="csproduct_gallery_prepare_layout">
        <observer name="video_template_set" instance="Ced\CsProduct\Observer\ChangeTemplate"/>
    </event>
    <event name="controller_action_predispatch_csmarketplace_vproducts_index">
        <observer name="csproduct_listing_active_after" instance="Ced\CsProduct\Observer\ChangeListLink"
                  shared="false"/>
    </event>
    <event name="controller_action_predispatch_csmarketplace_vproducts_new">
        <observer name="csproduct_form_active_after" instance="Ced\CsProduct\Observer\ChangeFormLink" shared="false"/>
    </event>
    <event name="ced_csmarketplace_predispatch_action">
        <observer name="ced_csproduct_set_rtl_in_vendorpanel"
                  instance="Ced\CsProduct\Observer\SetRtl" shared="false"/>
    </event>
</config>
